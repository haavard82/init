<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.admin.backupscript</string>

	<key>ProgramArguments</key>
	<array>
	<string>/bin/zsh</string>
	<string>/Users/admin/init/PeriodicBackup.sh</string>
	</array>

	<key>RunAtLoad</key>
	<true/>

	<key>KeepAlive</key>
	<true/>

    <key>StandardOutPath</key>
    <string>/Users/admin/init/logs/backupscript.out</string>

    <key>StandardErrorPath</key>
    <string>/Users/admin/init/logs/backupscript.err</string>	

</dict>
</plist>
